# Docker 环境配置
# 复制此文件为 .env 使用

# PostgreSQL 数据库配置（Docker容器）
DB_HOST=localhost
DB_PORT=5432
DB_NAME=fast_rag
DB_USER=postgres
DB_PASSWORD=password

# Ollama 模型配置
OLLAMA_MODEL=llama3:latest

# 应用配置
MAX_CONTEXT_CHARS=1200
MAX_GENERATE_TOKENS=800
NUM_CTX=2048
NUM_PREDICT=800

# Redis 配置（可选）
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=logs/app.log
